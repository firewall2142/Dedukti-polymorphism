def leibniz :
  cts.Term
    cts.diamond
    (cts.prod
       cts.triangle
       cts.box
       cts.diamond
       cts.I
       (cts.univ cts.box cts.triangle cts.I)
       (x =>
        cts.prod
          (cts.Term cts.triangle (cts.univ cts.box cts.triangle cts.I)) (;cts.box ;)
          cts.box
          cts.box
          cts.I
          x
          (x0 => cts.prod cts.box cts.box cts.box cts.I x (x1 => cts.univ cts.star cts.box cts.I)))).
